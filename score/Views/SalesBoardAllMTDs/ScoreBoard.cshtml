                animationEnabled: true,

                title: {
                    text: "@ReportTitle"
                },
                axisX: {
                    interval: 1,
                    labelFontSize: 18,
                },
                axisY2: {
                    interlacedColor: "rgba(1,77,101,.2)",
                    gridColor: "rgba(1,77,101,.1)",
                },
                data: [{
                    type: "bar",
                    showInLegend: "true",
                    name: "@ViewBag.legend1",
                    xValueFormatString: "string",
                    yValueFormatString: "#",
                    color: "#014D65",
                    dataPoints: [@Html.Raw(ViewBag.DataPoints)]
                },
                {
                    type: "bar",
                    showInLegend: "true",
                    name: "@ViewBag.legend2",
                    xValueFormatString: "string",
                    yValueFormatString: "#",
                    color: "IndianRed",
                    dataPoints: [@Html.Raw(ViewBag.DataPoints2)]
                },
                {
                    type: "bar",
                    showInLegend: "true",
                    name: "@ViewBag.legend3",
                    xValueFormatString: "string",
                    yValueFormatString: "#",
                    color: "SpringGreen",
                    dataPoints: [@Html.Raw(ViewBag.DataPoints3)]
                }
                ]
            });


                            chart.render();

                        }
    </script>
</head>
<body>
    <div id="chartContainer" style="height: 770px; max-width: 1320px; margin: 0px auto"></div>
    <script src="../scripts/canvasjs.min.js"></script>
</body>
</html>