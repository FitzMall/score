@model IEnumerable<score.Models.SalesBoardAllMTD>


@{
    ViewBag.Title = "Index";
    string Location = ViewBag.Location;
    string ReportDate = ViewBag.ReportDate;
    int DaysInMonth = ViewBag.DIM;
    var applicationPath = Request.ApplicationPath;
    if (applicationPath == "/")
    {
        applicationPath = "";
    }
    int inc = 0;

    string[] a_AssociateName = new string[80];
    decimal[] a_MTD = new decimal[80];
}
<!DOCTYPE HTML>
<html>
@foreach (var item in Model)
{
    a_AssociateName[inc] = item.FIRSTNAME.ToUpper() + " " + item.LASTNAME.ToUpper();
    a_MTD[inc] = ((decimal)item.MTD);
    inc += 1;
    if (inc > 80)
    {
        inc = 80;
    }
    
}
